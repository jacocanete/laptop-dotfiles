layout {
    default_tab_template {
        pane size=1 borderless=true {
            plugin location="tab-bar"
        }
        pane
        pane size=1 borderless=true {
            plugin location="status-bar"
        }
    }

    swap_floating_layout {
        floating_panes {
            pane {
                x "70%"
                y 0
                width "35%"
                height "100%"
            }
        }
    }
}
session_serialization false
